    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "draft-js": "^0.11.7",
    "express": "^4.18.1",
    "jest": "^29.1.1",
    "joi": "^17.6.1",
    "markdown-draft-js": "^2.4.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "react": "^18.2.0",
    "react-bootstrap": "0.33.1",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
